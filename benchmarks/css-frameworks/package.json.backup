{
  "name": "css-frameworks-benchmark",
  "version": "1.0.0",
  "description": "Professional CSS framework benchmark suite comparing runtime performance, CSS output size, and build times",
  "type": "module",
  "scripts": {
    "benchmark": "npm run bench:runtime && npm run bench:build && npm run merge:results && node ../../scripts/save-dated-result.cjs results",
    "bench:runtime": "vitest bench --run --outputJson=results/runtime.json",
    "bench:build": "tsx build-tests/run-all.ts",
    "merge:results": "tsx build-tests/merge-results.ts",
    "clean": "rm -rf results dist && mkdir -p results"
  },
  "dependencies": {
    "@sylphx/silk": "^1.0.0",
    "tailwindcss": "^3.4.15",
    "@pandacss/dev": "^1.4.3",
    "unocss": "^0.65.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.7",
    "vitest": "^4.0.7",
    "tsx": "^4.20.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
