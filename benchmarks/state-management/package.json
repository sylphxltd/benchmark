{
  "name": "state-management-benchmark",
  "version": "1.0.0",
  "description": "Professional state management benchmark suite using Vitest Bench",
  "type": "module",
  "scripts": {
    "benchmark": "vitest bench --run --outputJson=results/latest.json && node ../../scripts/save-dated-result.cjs results && node ../../scripts/generate-main-readme.cjs .",
    "benchmark:read": "node ../../scripts/run-group-benchmark.cjs 01-read .",
    "benchmark:write": "node ../../scripts/run-group-benchmark.cjs 02-write .",
    "benchmark:creation": "node ../../scripts/run-group-benchmark.cjs 03-creation .",
    "benchmark:complexity": "node ../../scripts/run-group-benchmark.cjs 04-complexity .",
    "benchmark:cache": "node ../../scripts/run-group-benchmark.cjs 05-cache .",
    "benchmark:memory": "node ../../scripts/run-group-benchmark.cjs 06-memory .",
    "benchmark:form": "node ../../scripts/run-group-benchmark.cjs 07-form .",
    "benchmark:async": "node ../../scripts/run-group-benchmark.cjs 08-async-reactive .",
    "benchmark:computed-native": "node ../../scripts/run-group-benchmark.cjs 09-computed-native .",
    "benchmark:selectors": "node ../../scripts/run-group-benchmark.cjs 10-selectors .",
    "benchmark:batching": "node ../../scripts/run-group-benchmark.cjs 11-batching-native .",
    "benchmark:universal": "npm run benchmark:read && npm run benchmark:write && npm run benchmark:creation && npm run benchmark:complexity && npm run benchmark:cache && npm run benchmark:memory && npm run benchmark:form",
    "benchmark:features": "npm run benchmark:async && npm run benchmark:computed-native && npm run benchmark:selectors && npm run benchmark:batching",
    "benchmark:all-groups": "npm run benchmark:universal && npm run benchmark:features",
    "clean": "rm -rf results && rm -rf groups/*/results.json && mkdir -p results"
  },
  "dependencies": {
    "@preact/signals": "2.4.0",
    "@reduxjs/toolkit": "^2.10.1",
    "@sylphx/zen": "^2.0.0",
    "jotai": "^2.15.1",
    "mobx": "^6.15.0",
    "recoil": "^0.7.7",
    "solid-js": "^1.9.10",
    "valtio": "2.2.0",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "tsx": "^4.20.6",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
